@using Shop.Net.Resources
@model ICollection<Shop.Net.Web.Areas.Catalog.Models.Product.ProductThumbnailModel>
@{
    ViewBag.Title = "Home Products Page";
}

<div class="jumbotron">
    <h1>@GlobalConstants.ShopName</h1>
    <p class="lead">Welcome to our shop! These are the lates items added</p>
</div>

<div class="row" id="thumbnails">
    @foreach (var product in Model)
    {
        @Html.Partial("_ThumbnailPartial", product);
    }
</div>